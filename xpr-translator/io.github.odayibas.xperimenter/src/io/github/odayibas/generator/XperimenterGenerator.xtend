	protected def getCount(Design design, VariableGroup argGroup) {
		var count = 0;
		for(item : design.variables)
		{
			if (item.group.equals(argGroup))
				count++;
		}
		return count;
	}
	
	protected def getFactorString(Design design) {
		var str = "c("
		var isFirst = true
		for (Variable v : design.variables.filter[group.equals(VariableGroup.FACTOR)])
		{
			if(isFirst)
				str += "2"
			else
				str += ",2"
			isFirst = false;
		}
		return str + ")";
	}
	
	protected def getFactorNameString(Design design) {
		var str = "c("
		var isFirst = true
		for (Variable v : design.variables.filter[group.equals(VariableGroup.FACTOR)])
		{
			if(isFirst)
				str += "&quot;" + v.name + "&quot;"
			else
			{
				str += ",&quot;" + v.name + "&quot;" 
			}
				
			isFirst = false
		}
		return str + ")"
	}
	
	protected def getFactorName(Design design, int i) {
		return design.variables.get(i).name
	}
	
	protected def getFactorLowHighStr(Design design) {
		var str = ""
		for (Variable v : design.variables.filter[group.equals(VariableGroup.FACTOR)])
		{
			str += "designMatrix$" + v.name + " &lt;- gsub(&quot;-1&quot;, &quot;l&quot;, designMatrix$" + v.name + ")&#10;designMatrix$" + v.name + " &lt;- gsub(&quot;1&quot;, &quot;h&quot;, designMatrix$" + v.name + ")&#10;&#10;" 
			str += "designMatrix$" + v.name + " &lt;- gsub(&quot;l&quot;, &quot;" + v.lowValue + "&quot;, designMatrix$" + v.name + ")&#10;designMatrix$" + v.name + " &lt;- gsub(&quot;h&quot;, &quot;" + v.highValue + "&quot;, designMatrix$" + v.name + ")&#10;&#10;"
		}
		return str
	}
	
