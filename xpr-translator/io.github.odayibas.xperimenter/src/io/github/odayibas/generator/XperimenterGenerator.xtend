	protected def toRCode(Experiment exp) {
		var factor_count = getCount(exp.design, VariableGroup.FACTOR)
		var response_count = getCount(exp.design, VariableGroup.RESPONSE)
		var nuisance_count = getCount(exp.design, VariableGroup.NUISANCE)
		switch exp.simulation.modelType {
			case CONTINUOUS: ''' CONT «factor_count»'''
			case DISCRETEEVENT: ''' SDF / F: «factor_count» / R: «response_count» / N: «nuisance_count» / str: «getFactorString(exp.design)» / name: «getFactorNameString(exp.design)»'''
			case STATIC: ''' STA «factor_count»'''
		}
	}
